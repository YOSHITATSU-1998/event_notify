# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=eyJxxxxx...your-supabase-key

# Slack Webhook
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/xxx/xxx/xxx

# Optional: LINE Notification Token
LINE_TOKEN=your-line-token-if-needed

# Optional: Manual Password
MANUAL_PASSWORD=your-manual-password

# Database Settings
ENABLE_DB_SAVE=1

# GitHub Pages (set to false for local execution)
GITHUB_PAGES=false
